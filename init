    @GetMapping("init")
    public void init(){
        Gouvernorat g ;
        Ville v ;


        g = new Gouvernorat();
        v = new Ville();
        v.setGouvernorats(g);
        v.setName("hbib");
        g.setName("tunis");
        villeRepo.save(v);

        g = new Gouvernorat();
        v = new Ville();
        v.setGouvernorats(g);
        v.setName("wed march");
        g.setName("bizerte");
        villeRepo.save(v);

        g = new Gouvernorat();
        v = new Ville();
        v.setGouvernorats(g);
        v.setName("birine");
        g.setName("sfax");
        villeRepo.save(v);

        g = new Gouvernorat();
        v = new Ville();
        v.setGouvernorats(g);
        v.setName("fouchena");
        g.setName("ben arous");
        villeRepo.save(v);

        g = gouvernoratRepo.findGouvernoratByName("bizerte");
        villeRepo.save(new Ville("andalos" , g));
        villeRepo.save(new Ville("blassa" , g));
        villeRepo.save(new Ville("sour" , g));
        villeRepo.save(new Ville("el kssar" , g));


         g = gouvernoratRepo.findGouvernoratByName("sfax");
        villeRepo.save(new Ville("tej" , g));
        villeRepo.save(new Ville("xxsf" , g));
        villeRepo.save(new Ville("ttsf" , g));
        villeRepo.save(new Ville("rrsf" , g));

         g = gouvernoratRepo.findGouvernoratByName("ben arous");
        villeRepo.save(new Ville("mhamdia" , g));
        villeRepo.save(new Ville("mourouj" , g));
        villeRepo.save(new Ville("ben arous" , g));
        villeRepo.save(new Ville("sidi hssin" , g));

    } 
